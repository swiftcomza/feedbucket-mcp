{
  "name": "feedbucket-mcp",
  "version": "1.0.1",
  "description": "MCP server for managing Feedbucket website feedback in Claude Code and Cursor",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "feedbucket-mcp": "./dist/index.js",
    "feedbucket-setup": "./dist/setup.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && node scripts/bundle.js",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "setup": "tsx src/setup.ts",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "feedbucket",
    "feedback",
    "claude",
    "cursor",
    "ai-assistant"
  ],
  "author": "SwiftCom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swiftcomza/feedbucket-mcp.git"
  },
  "homepage": "https://github.com/swiftcomza/feedbucket-mcp#readme",
  "bugs": {
    "url": "https://github.com/swiftcomza/feedbucket-mcp/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=18"
  }
}